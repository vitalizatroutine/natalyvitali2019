{
  "name": "natalyvitali2019",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "history": "4.7.2",
    "immutability-helper": "2.7.0",
    "lodash": "4.17.5",
    "moment": "2.21.0",
    "moment-timezone": "0.5.14",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "4.1.2",
    "prop-types": "15.6.0",
    "query-string": "5.1.0",
    "react": "16.3.0",
    "react-custom-scrollbars": "4.2.1",
    "react-datepicker": "1.5.0",
    "react-dom": "16.3.3",
    "react-ga": "2.5.3",
    "react-redux": "5.0.7",
    "react-render-html": "0.6.0",
    "react-router-dom": "4.2.2",
    "react-router-redux": "next",
    "react-scripts": "1.1.1",
    "react-transition-group": "2.2.1",
    "react-truncate": "2.3.0",
    "redux": "3.7.2",
    "redux-logger": "3.0.6",
    "redux-persist": "5.9.1",
    "redux-thunk": "2.2.0",
    "url-search-params-polyfill": "2.0.3"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "build:dev": "REACT_APP_ENV=development npm-run-all build-css build-js",
    "build:stage": "REACT_APP_ENV=staging npm-run-all build-css build-js",
    "build:preprod": "REACT_APP_ENV=pre_production npm-run-all build-css build-js",
    "build:prod": "REACT_APP_ENV=production npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "eject": "react-scripts eject"
  }
}
